{
  "editor.defaultFormatter": "svipas.prettier-plus",
  "files.associations": {
    "*.xml": "html",
    "*.svg": "html"
  },
  "editor.formatOnSave": true,
  "[svg]": {
    "editor.defaultFormatter": "svipas.prettier-plus",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "svipas.prettier-plus"
  }
}
